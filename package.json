{
  "name": "pain-reporting-portal",
  "version": "0.1.0",
  "description": "Pain Reporting Portal",
  "main": "index.js",
  "scripts": {
    "describe-api": "pm2 describe prp-api",
    "describe-dashboard": "pm2 describe prp-dashboard",
    "describe-scheduler": "pm2 describe prp-scheduler",
    "documentation": "jsdoc --recurse api controller model rule task --destination documentation",
    "init": "node task/init",
    "postinstall": "bower install",
    "lint": "eslint . view/**/*.hbs --inline-config=false && remark *.md && stylelint view/stylesheet/*.css",
    "lint-fix": "eslint . --fix --inline-config=false",
    "log": "pm2 logs",
    "outdated": "npm outdated && bower list",
    "preseed": "node task/sync",
    "seed": "node task/seed",
    "start": "pm2 start task/start-api.json && pm2 start task/start-dashboard.json && pm2 start task/start-scheduler.json",
    "start-api": "pm2 start task/start-api.json",
    "start-dashboard": "pm2 start task/start-dashboard.json",
    "start-scheduler": "pm2 start task/start-scheduler.json",
    "status": "pm2 ls",
    "stop": "pm2 delete prp-api && pm2 delete prp-dashboard && pm2 delete prp-scheduler",
    "stop-api": "pm2 delete prp-api",
    "stop-dashboard": "pm2 delete prp-dashboard",
    "stop-scheduler": "pm2 delete prp-scheduler",
    "sync": "node task/sync",
    "pretest": "node task/sync-test && node task/seed-test",
    "test": "ava */test/*.js",
    "user": "node task/user"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/promisasu/Portal.git"
  },
  "keywords": [
    "medical",
    "portal"
  ],
  "author": "Arizona State University",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/promisasu/Portal/issues"
  },
  "homepage": "https://github.com/promisasu/Portal#readme",
  "dependencies": {
    "boom": "^3.1.2",
    "bower": "^1.7.6",
    "handlebars": "^4.0.5",
    "hapi": "^13.0.0",
    "hapi-auth-basic": "^4.1.0",
    "inert": "^3.2.0",
    "joi": "^7.2.3",
    "moment": "^2.11.2",
    "mysql": "^2.10.2",
    "sequelize": "^3.19.2",
    "vision": "^4.0.1"
  },
  "optionalDependencies": {
    "bcrypt": "^0.8.5"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "eslint": "^2.0.0-rc.0",
    "eslint-plugin-html": "^1.3.0",
    "jsdoc": "^3.4.0",
    "pm2": "^1.0.0",
    "read": "^1.0.7",
    "remark": "^3.2.3",
    "remark-lint": "^2.3.1",
    "remark-slug": "^4.0.0",
    "remark-validate-links": "^2.0.2",
    "stylelint": "^4.2.0"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  }
}
